{
  "name": "vite-plugin-mock",
  "version": "2.1.2",
  "description": "A mock plugin for vite",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.es.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.es.json",
    "prepublishOnly": "yarn build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "vite",
    "mock",
    "hmr"
  ],
  "author": "Vben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anncwb/vite-plugin-mock"
  },
  "bugs": {
    "url": "https://github.com/anncwb/vite-plugin-mock/issues"
  },
  "homepage": "https://github.com/anncwb/vite-plugin-mock/tree/master/#readme",
  "dependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/mockjs": "^1.0.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "connect": "^3.7.0",
    "dayjs": "^1.10.4",
    "debug": "^4.3.2",
    "esbuild": "^0.8.39",
    "fast-glob": "^3.2.5",
    "rollup": "^2.38.4",
    "rollup-plugin-esbuild": "^2.6.1"
  },
  "peerDependencies": {
    "mockjs": ">=1.1.0",
    "vite": ">=2.0.0-beta.36"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.24",
    "conventional-changelog-cli": "^2.1.1",
    "mockjs": "^1.1.0",
    "rimraf": "^3.0.2",
    "tsup": "^3.12.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.64"
  }
}
